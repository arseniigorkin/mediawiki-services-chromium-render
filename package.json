    "url": "https://phabricator.wikimedia.org/tag/proton/"
  },
  "homepage": "https://github.com/wikimedia/mediawiki-services-chromium-render",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "cassandra-uuid": "^0.0.2",
    "compression": "^1.7.1",
    "domino": "^1.0.30",
    "express": "^4.16.2",
    "http-shutdown": "^1.2.0",
    "js-yaml": "^3.10.0",
    "preq": "^0.5.6",
    "puppeteer-core": "1.20.0",
