  "devDependencies": {
    "ajv": "^5.5.0",
    "eslint": "^4.12.0",
    "eslint-config-node-services": "^2.2.5",
    "eslint-config-wikimedia": "^0.5.0",
    "eslint-plugin-jsdoc": "^3.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "extend": "^3.0.1",
    "json-schema-ref-parser-sync": "^1.0.0",
    "mocha": "^4.0.1",
    "mocha-eslint": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "15.0.0",
